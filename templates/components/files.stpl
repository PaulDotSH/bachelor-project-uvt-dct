<%
use crate::constants::*;
%>
<label>Files:</label>
<% for file in files { %>
    <div><a href="<%- ASSETS_CLASSES_PATH %>/<%= file.name %>"><%= file.name %></a></div>
    <form action="/files/<%- file.id %>/delete" method="POST">
        <input type="submit" value="Delete file" />
    </form>
<% } %>